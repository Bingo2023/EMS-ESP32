    "@mui/material": "^5.11.9",
    "@remix-run/router": "^1.3.2",
    "@table-library/react-table-library": "4.0.25",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.14.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-router-dom": "^5.3.3",
